      "credentials": [
        {
          "type": "password",
          "value": "admin",
          "temporary": false
        }
      ],
      "realmRoles": ["default-roles-kubezen"]
    }
  ],
  "roles": {
    "realm": [
      {
        "name": "kubezen-admin",
