{
  "name": "kubezen",
  "private": true,
  "version": "0.1.0",
  "description": "KubeZen monorepo workspace",
  "license": "MIT",
  "packageManager": "pnpm@10.24.0",
  "scripts": {
    "dev": "pnpm -C web dev",
    "dev:backend": "go run ./cmd/server",
    "dev:frontend": "pnpm -C web dev",
    "build": "pnpm -C web build",
    "lint": "pnpm -C web lint",
    "typecheck": "pnpm -C web typecheck",
    "format": "pnpm -C web format",
    "docker:keycloak:build": "docker build -t kubezen-keycloak:dev ./docker/keycloak",
    "docker:keycloak": "docker run -d --rm -p 8081:8080 -e KC_BOOTSTRAP_ADMIN_USERNAME=admin -e KC_BOOTSTRAP_ADMIN_PASSWORD=admin kubezen-keycloak:dev"
  },
  "dependencies": {
    "@radix-ui/react-select": "^2.2.6"
  }
}
